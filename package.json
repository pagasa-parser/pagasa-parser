{
    "name": "pagasa-parser",
    "version": "2.0.0-rc1",
    "description": "A Node.js library that parses information from PAGASA's Severe Weather Bulletin page and turns it into various formats.",
    "main": "build/PagasaParser.js",
    "dependencies": {
        "string-similarity": "^4.0.4"
    },
    "devDependencies": {
        "@types/jest": "^27.0.1",
        "@types/string-similarity": "^4.0.0",
        "@typescript-eslint/eslint-plugin": "^4.6.0",
        "cat": "^0.2.0",
        "eslint": "^7.12.1",
        "jest": "^27.1.0",
        "ts-jest": "^27.0.5",
        "ts-node": "^10.2.1",
        "typescript": "^4.4.2"
    },
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "coverage": "npm run coverage:full",
        "coverage:full": "jest --coverage --collectCoverageFrom=src/**/*.{js,jsx,ts,tsx}",
        "coverage:minimal": "jest --coverage",
        "reignore": "cat .gitignore > .npmignore && cat .gitignore.npmignore >> .npmignore",
        "prepack": "npm run build && npm run reignore"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ChlodAlejandro/PAGASA-parser.git"
    },
    "keywords": [
        "scraper",
        "weather",
        "json",
        "philippines",
        "typhoon"
    ],
    "author": "Chlod Alejandro <chlod@chlod.net> (https://chlod.net)",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/ChlodAlejandro/PAGASA-parser/issues"
    },
    "homepage": "https://github.com/ChlodAlejandro/PAGASA-parser#readme"
}
